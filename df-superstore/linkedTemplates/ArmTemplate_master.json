{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "df-superstore"
		},
		"ls_AmazonS3_SuperStore_secretAccessKey": {
			"type": "secureString",
			"metadata": "Secure string for 'secretAccessKey' of 'ls_AmazonS3_SuperStore'"
		},
		"ls_azureblobstorage_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'ls_azureblobstorage'"
		},
		"ls_AmazonS3_SuperStore_properties_typeProperties_accessKeyId": {
			"type": "string",
			"defaultValue": "AKIAWMFUPAU7Y4KTVVVP"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "df-superstore_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"ls_AmazonS3_SuperStore_secretAccessKey": {
						"value": "[parameters('ls_AmazonS3_SuperStore_secretAccessKey')]"
					},
					"ls_azureblobstorage_connectionString": {
						"value": "[parameters('ls_azureblobstorage_connectionString')]"
					},
					"ls_AmazonS3_SuperStore_properties_typeProperties_accessKeyId": {
						"value": "[parameters('ls_AmazonS3_SuperStore_properties_typeProperties_accessKeyId')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}